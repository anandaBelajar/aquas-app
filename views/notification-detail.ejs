<%- include('partials/header') %>
    <div class="container body">

        <div class="main_container">
            <div class="col-md-3 left_col">
                <%- include('partials/sidebar-menu') %>
            </div>

            <%- include('partials/top-navigation') %>

                <!-- page content -->
                <div class="right_col" role="main">
                    <!-- top tiles -->
                    <div class="page-title">
                        <div class="title_left">
                            <h3>Kelola Admin</h3>
                        </div>
                    </div>
                    <div class="clearfix"></div>

                    <!-- /top tiles -->
                    <!--Start Administrators table-->
                    <div class="row administrator_list_container">

                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph notification-detail-container">
                                <%
var raw_date = String(item[0].tanggal),
    date = raw_date.split(" "),
    raw_time = date[5].split("+"),
    time = raw_time[1].split("")

     
%>
                                    <div class="row x_title">
                                        <div class="col-md-6">
                                            <h3>
                                                <%= item[0].jenis %>
                                            </h3>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="pull-right">
                                                <i class="fa fa-calendar"></i>
                                                <span>
                                                <%= date[2]; %>
                                                <%= date[1]; %>
                                                <%= date[3]; %>, 
                                            </span>
                                                <span>Pukul : 
                                                    <%= time[0] %>
                                                    <%= time[1] %> :
                                                    <%= time[2] %>
                                                    <%= time[3] %>
                                                    </span> <b class="caret"></b>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12 col-sm-12 notification-full-description">
                                        <%= item[0].deskripsi %>
                                    </div>
                                    <div class="clearfix"></div>
                            </div>
                        </div>

                        <br />
                    </div>
                    <!--End Administrators Tables-->
                    <br />

                    <!-- /top tiles -->

                </div>
                <!-- /page content -->

                <%- include('partials/footer') %>
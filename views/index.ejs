<%- include('partials/header') %>
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <%- include('partials/sidebar-menu') %>
            </div>

            <%- include('partials/top-navigation') %>

                <!-- page content -->
                <div class="right_col" role="main">
                    <!-- top tiles -->
                    <div class="row" style="display: inline-block;">
                        <div class="tile_count">
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-user"></i> Total Users</span>
                                <div class="count">2500</div>
                                <span class="count_bottom"><i class="green">4% </i> From last Week</span>
                            </div>
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-clock-o"></i> Average Time</span>
                                <div class="count">123.50</div>
                                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>3% </i> From last Week</span>
                            </div>
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-user"></i> Total Males</span>
                                <div class="count green">2,500</div>
                                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
                            </div>
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-user"></i> Total Females</span>
                                <div class="count">4,567</div>
                                <span class="count_bottom"><i class="red"><i class="fa fa-sort-desc"></i>12% </i> From last Week</span>
                            </div>
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-user"></i> Total Collections</span>
                                <div class="count">2,315</div>
                                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
                            </div>
                            <div class="col-md-2 col-sm-4  tile_stats_count">
                                <span class="count_top"><i class="fa fa-user"></i> Total Connections</span>
                                <div class="count">7,325</div>
                                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
                            </div>
                        </div>
                    </div>

                    <!-- /top tiles -->
                    <!--Start Feed Chart-->
                    <div class="row feed_info_container">
                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph">

                                <div class="row x_title">
                                    <div class="col-md-6">
                                        <h3>Sisa Pakan</h3>
                                    </div>
                                </div>

                                <div class="col-md-9 col-sm-9 ">
                                    <div class="progress">
                                        <div id="aquas_feed_bar" class="progress-bar" role="progressbar" style="width: 75%;" aria-valuenow="75">75%</div>
                                    </div>
                                    <!--<div id="chart_plot_01" class="demo-placeholder"></div>-->
                                </div>
                                <div class="col-md-3 col-sm-3  bg-white">
                                    <div class="col-md-12 col-sm-12 ">

                                        <div class="x_title">
                                            <h2>Konsol Pakan</h2>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="aquas_auto_feed_toggle_container">
                                            <div class="custom-control custom-switch custom-switch-md">
                                                <input type="checkbox" class="custom-control-input" id="aquas_auto_feed_toggle">
                                                <label class="custom-control-label" for="aquas_auto_feed_toggle">Manual</label>
                                            </div>
                                            <p class="control_description">Aktifkan kontrol pakan otomatis</p>
                                        </div>
                                        <div class="aquas_manual_feed_toggle_container">
                                            <div class="custom-control custom-switch custom-switch-md">
                                                <input type="checkbox" class="custom-control-input" id="aquas_manual_feed_toggle">
                                                <label class="custom-control-label" for="aquas_manual_feed_toggle">tutup</label>
                                            </div>
                                            <p class="control_description">Buka atau tutup container pakan</p>
                                        </div>

                                    </div>

                                </div>

                                <div class="clearfix"></div>
                            </div>
                        </div>

                    </div>
                    <!--End Feed Chart-->
                    <br />

                    <!-- /top tiles -->
                    <!--Start Light Chart-->
                    <div class="row light_info_container">
                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph">

                                <div class="row x_title">
                                    <div class="col-md-6">
                                        <h3>Intensitas Cahaya</h3>
                                    </div>
                                </div>

                                <div class="col-md-9 col-sm-9 ">
                                    <canvas id="aquas_light_chart"></canvas>
                                    <!--<div id="chart_plot_01" class="demo-placeholder"></div>-->
                                </div>
                                <div class="col-md-3 col-sm-3  bg-white">
                                    <div class="x_title">
                                        <h2>Intensitas Cahaya Realtime</h2>
                                        <div class="clearfix"></div>
                                    </div>

                                    <div class="col-md-12 col-sm-12 ">
                                        <div>
                                            <div class="current_value_wrapper">
                                                <span id="aquas_light_current_value" class='aquas_current_value'></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12 col-sm-12 ">

                                        <div class="x_title">
                                            <h2>Konsol Pencahayaan</h2>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="aquas_auto_light_toggle_container">
                                            <div class="custom-control custom-switch custom-switch-md">
                                                <input type="checkbox" class="custom-control-input" id="aquas_auto_light_toggle">
                                                <label class="custom-control-label" for="aquas_auto_light_toggle">Manual</label>
                                            </div>
                                            <p class="control_description">Aktifkan kontrol pencahyaan otomatis</p>
                                        </div>
                                        <div class="aquas_manual_light_toggle_container">
                                            <div class="custom-control custom-switch custom-switch-md">
                                                <input type="checkbox" class="custom-control-input" id="aquas_manual_light_toggle">
                                                <label class="custom-control-label" for="aquas_manual_light_toggle">Off</label>
                                            </div>
                                            <p class="control_description">Hidupkan atau matikan growlight</p>
                                        </div>

                                    </div>

                                </div>

                                <div class="clearfix"></div>
                            </div>
                        </div>

                    </div>
                    <!--End Light Chart-->
                    <br />

                    <!--Start Temp Chart-->
                    <div class="row temp_info_container">
                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph">

                                <div class="row x_title">
                                    <div class="col-md-6">
                                        <h3>Suhu Air</h3>
                                    </div>
                                </div>

                                <div class="col-md-9 col-sm-9 ">
                                    <canvas id="aquas_temp_chart"></canvas>
                                </div>
                                <div class="col-md-3 col-sm-3  bg-white">
                                    <div class="x_title">
                                        <h2>Suhu Air Realtime</h2>
                                        <div class="clearfix"></div>
                                    </div>

                                    <div class="col-md-12 col-sm-12 ">
                                        <div>
                                            <div class="current_value_wrapper">
                                                <span id="aquas_temp_current_value" class='aquas_current_value'></span>
                                            </div>
                                        </div>
                                    </div>


                                </div>


                                <div class="clearfix"></div>
                            </div>
                        </div>

                    </div>
                    <!--End Temp Chart-->
                    <br />

                    <!--Start pH Chart-->
                    <div class="row ph_info_container">
                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph">

                                <div class="row x_title">
                                    <div class="col-md-6">
                                        <h3>Derajat pH Air</h3>
                                    </div>
                                </div>

                                <div class="col-md-9 col-sm-9 ">
                                    <canvas id="aquas_ph_chart"></canvas>
                                </div>
                                <div class="col-md-3 col-sm-3  bg-white">
                                    <div class="x_title">
                                        <h2>pH Air Realtime</h2>
                                        <div class="clearfix"></div>
                                    </div>

                                    <div class="col-md-12 col-sm-12 ">
                                        <div>
                                            <div class="current_value_wrapper">
                                                <span id="aquas_ph_current_value" class='aquas_current_value'></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>


                                <div class="clearfix"></div>
                            </div>
                        </div>

                    </div>
                    <!--End pH Chart-->
                    <br />
                </div>
                <!-- /page content -->

                <%- include('partials/footer') %>